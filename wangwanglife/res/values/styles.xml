<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppBaseTheme" parent="android:Theme.Light"></style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <!-- 旺旺主题 -->
    <style name="ww_app_theme" parent="android:Theme.Light.NoTitleBar">

        <!-- 默认textview的字体颜色 -->
        <item name="android:textColorSecondary">@color/default_text_color</item>
    </style>

    <!-- 水平分隔线 -->
    <style name="cutline_fine">
        <item name="android:background">@color/gray_line</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.6dp</item>
    </style>
    <!-- 垂直分隔线 -->
    <style name="cutline_fine_v">
        <item name="android:background">@color/gray_line</item>
        <item name="android:layout_width">0.6dp</item>
        <item name="android:layout_height">match_parent</item>
    </style>

</resources>